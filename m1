#include <stdio.h>
#include <stdlib.h>
#include <string.h>

typedef struct FlightNode
{
	char* destination;
	char* date;
	float fare;
	struct FlightNode* prevFare;
	struct FlightNode* nextFare;
	struct FlightNode* prevDest;
	struct FlightNode* nextDest;
};
